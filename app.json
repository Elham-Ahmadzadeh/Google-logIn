{
  "expo": {
    "name": "teamfrida",
    "slug": "teamfrida",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "teamfrida",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#23EDF9"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
